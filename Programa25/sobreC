int teste1(int valor1, int valor2)
{
    if (valor1 > valor2)
    {
        return valor1;
    }
    else
    {
        return valor2;
    }
}

int teste2(int valor1)
{
    int ret = 0;
    switch (valor1)
    {
    case 1:
        ret = 5;
        break;
    case 2:
        ret = 6;
        break;
    case 3:
        ret = 4;
        break;
    case 4:
        ret = 5;
        break;
    }
    return ret;
}
